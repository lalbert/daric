{
	"name" : "lalbert/daric",
	"description" : "Simple and configurable PHP web spider and web scraper",
	"type" : "library",
	"license" : "MIT",
	"authors" : [{
			"name" : "Luc ALBERT",
			"homepage" : "https://github.com/orgs/lalbert/people/lucalbert"
		}
	],
	"keywords" : [
		"scraper",
		"spider"
	],
	"require" : {
		"fabpot/goutte" : "~3.1",
		"jakeasmith/http_build_url" : "~1.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~5.3"
	},
	"autoload" : {
		"psr-4" : {
			"Daric\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Daric\\Tests\\" : [
				"tests/unit"
			]
		}
	}
}
